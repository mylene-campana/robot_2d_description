<?xml version="1.0"?>
<robot name="cylinder_obstacle"> <!-- simple obstacle-->
  <link name="obstacle_base">
    <inertial>
      <origin xyz="0 0 0" /> 
      <mass value="1.0" />
      <inertia  ixx="1.0" ixy="0.0"  ixz="0.0"  iyy="100.0"  iyz="0.0"  izz="1.0" />
    </inertial>
    <visual>
      <origin xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.4" radius="0.5"/>
      </geometry>
      <material name="Red">
        <color rgba="0.8 0 0 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.4" radius="0.5"/>
      </geometry>
    </collision>
  </link>
</robot>
